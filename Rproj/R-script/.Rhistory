names(test.0)
post<-test.0$posterior
names(post)
postdist.beta.hat <- post$beta$sample
postdist.sigmasq.hat <- post$sigmasq$sample
postdist.phi.hat <- post$phi$sample
par(mfrow=c(1,3))
hist(postdist.beta.hat)
hist(postdist.sigmasq.hat)
